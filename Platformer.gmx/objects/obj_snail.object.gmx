            <kind>1</kind>
            <string>///snail things

//reverse direction if colliding

if (mvleft &amp;&amp; !position_meeting(x-objsize/2,y,obj_wall)){
    hspd = -spd;
} else if (mvleft &amp;&amp; position_meeting(x-objsize/2,y,obj_wall)){
    mvleft = false;
} else if (!mvleft &amp;&amp; position_meeting(x+objsize/2,y,obj_wall)){
    mvleft = true;
} else if (!mvleft &amp;&amp; !position_meeting(x+objsize/2,y,obj_wall)){
    hspd = spd;
}


//leave slime
if (place_snapped(objsize, objsize)){
    instance_create(x,y,obj_slime);
}

//when snail meets guy
if (position_meeting(x,y-objsize/2,obj_player) &amp;&amp; obj_player.y &lt; y-1){
        obj_player.vspd = -obj_player.jspd;
        instance_destroy();
}else if(position_meeting(x+objsize/2,y+1,obj_player)||position_meeting(x-objsize/2,y+1,obj_player)){
        with (obj_player) instance_destroy();
}

//move snail
x+=hspd;
</string>
